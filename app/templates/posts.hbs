<div class=" jumbotron text-center">
  <h1>Reviews</h1>
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Message</th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |message|}}
        <tr>
          <td>{{message.name}}</td>
          <td>{{message.message}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
